# name: gen_server
# key: gen_server
# --
init(S)->
     {ok, S}
    .
    
handle_call(_Request,_From,State)->
    {noreply, State}
        .

handle_cast(_Request,State)->
    {noreply, State} .

handle_info(_Info,State)->
    {noreply, State}.

terminate(_Reason,_State)->
      ok .

code_change(Previous_Version,State,Extra)->
     {ok,State} .