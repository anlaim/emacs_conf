# name: gen_server
# key: gen_server
# --

start_link()->
    gen_server:start_link(?MODULE,hello,[])
.

init(S)->
     {ok, S}
    .
    
handle_call(_Request,_From,State)->
    {noreply, State}
        .

handle_cast(_Request,State)->
    {noreply, State} .

handle_info(_Info,State)->
    {noreply, State}.

terminate(_Reason,_State)->
      ok .

code_change(_Previous_Version,State,_Extra)->
     {ok,State} .